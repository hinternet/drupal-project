<VirtualHost *:80>
  ServerAdmin admin@docker.es
  ServerName drupal.docker.localhost

  DocumentRoot /var/www/html/drupalmitma/web

  <Directory /var/www/html/drupalmitma/web>
    Options FollowSymLinks
    AllowOverride All
    Require all granted
  </Directory>
  ServerSignature Off

  ErrorLog ${APACHE_LOG_DIR}/error.log
  LogLevel warn
  CustomLog ${APACHE_LOG_DIR}/access.log combined

</VirtualHost>
